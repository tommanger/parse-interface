{
  "compilerOptions": {
    "module": "commonjs",
    "alwaysStrict": true,
    "importHelpers": true,
    "target": "es2018",
    "noImplicitAny": false,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "allowJs": true,
    "preserveConstEnums": true
  }
}
